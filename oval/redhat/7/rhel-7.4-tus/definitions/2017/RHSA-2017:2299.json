  "Metadata": {
    "Title": "RHSA-2017:2299: NetworkManager and libnl3 security, bug fix and enhancement update (Moderate)",
    "AffectedList": [
      {
        "Family": "unix",
        "Platforms": [
          "Red Hat Enterprise Linux 7"
        ]
      }
    ],
    "References": [
