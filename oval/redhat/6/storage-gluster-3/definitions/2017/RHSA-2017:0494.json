  "Metadata": {
    "Title": "RHSA-2017:0494: Red Hat Gluster Storage 3.2.0 samba security, bug fixes and enhancement update (Moderate)",
    "AffectedList": [
      {
        "Family": "unix",
        "Platforms": [
          "Red Hat Gluster Storage 3.2 for RHEL 6"
        ]
      }
    ],
    "References": [
      {
        "Source": "RHSA",
        "RefID": "RHSA-2017:0494",
        "RefURL": "https://access.redhat.com/errata/RHSA-2017:0494"
      },
      {
        "Source": "CVE",
        "RefID": "CVE-2016-2125",
        "RefURL": "https://access.redhat.com/security/cve/CVE-2016-2125"
      },
